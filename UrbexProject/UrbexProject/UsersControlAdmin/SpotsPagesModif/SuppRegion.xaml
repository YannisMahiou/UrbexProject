<Window x:Class="UrbexProject.UsersControlAdmin.SpotsPagesModif.SuppRegion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UrbexProject.UsersControlAdmin.SpotsPagesModif"
        mc:Ignorable="d"
        Title="SuppRegion" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Content="Supprimer une Région" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.ColumnSpan="2" FontSize="20" FontWeight="Bold"/>
        <StackPanel Grid.Row="1" Grid.ColumnSpan="2" Margin="30,0" VerticalAlignment="Center">
            <Label Content="Nom de la région"/>
            <TextBox x:Name="NomRegion"/>
        </StackPanel>
        <Button Grid.Row="2" Margin="30,30" Content="Annuler" Click="Annuler"/>
        <Button Grid.Row="2" Grid.Column="1" Margin="30,30" Content="Valider" Click="Valider"/>
    </Grid>
</Window>
