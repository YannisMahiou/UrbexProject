<Window x:Class="UrbexProject.UsersControlAdmin.SpotsPagesModif.AddRegion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UrbexProject.UsersControlAdmin.SpotsPagesModif"
        mc:Ignorable="d"
        Title="AddRegion" Height="350" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="3*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Grid.ColumnSpan="2" Content="Ajouter une Région" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" FontSize="23"/>
        <StackPanel Grid.Row="1" Grid.ColumnSpan="2">
            <Label Content="Nom De la Région" Margin="30,0"/>
            <TextBox x:Name="NomRegion" Margin="30,0"/>
            <Label Content="Description De la Région" Margin="30,0"/>
            <TextBox x:Name="DescriptionRegion" Margin="30,0" Height="50"/>
            <Label Content="Image de la Région" Margin="30,0"/>
            <Button Content="Ouvrir la galerie" Margin="30,0" Click="ouvrirGalerie"/>
            <TextBlock x:Name="retourGalerie" Margin="30,0" FontSize="11" FontStyle="Italic"/>
        </StackPanel>
        <Button Grid.Row="3" Margin="30,20" Content="Annuler" Click="Annuler"/>
        <Button Grid.Row="3" Grid.Column="1" Margin="30,20" Content="Valider" Click="Valider"/>
    </Grid>
</Window>
